#!/bin/bash
up=$(uptime -p | sed 's/up //')
lastboot=$(uptime -s)

echo "                              [1;90mâ• â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•[[1;39m   Uptime    [1;90m]â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•[0m"
echo "                              [1;90mâ•‘ [1;31muptime[0m....: ${up}"
echo "                              [1;90mâ•‘ [1;31mlast_boot[0m.: ${lastboot}"
